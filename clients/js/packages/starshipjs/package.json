{
  "name": "starshipjs",
  "version": "3.0.0",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "description": "JS utilities for Starship",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "homepage": "https://github.com/hyperweb-io/starship",
  "license": "SEE LICENSE IN LICENSE",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
